# Copy to .env (or set in Lambda env vars). Keep sandbox-only values.

AWS_REGION=ap-southeast-2
LOG_LEVEL=INFO

# Common SFMC
SFMC_SECRET_ARN=__REPLACE_ME__   # or SFMC_SECRET_ID
SFMC_ALLOWED_ACCOUNT_ID=__REPLACE_ME__

# Optional SFMC base URLs (if not in secret)
# SFMC_AUTH_BASE_URL=https://YOUR_SUBDOMAIN.auth.marketingcloudapis.com
# SFMC_REST_BASE_URL=https://YOUR_SUBDOMAIN.rest.marketingcloudapis.com

# Optional timeouts
SFMC_TOKEN_TIMEOUT=20
SFMC_API_TIMEOUT=30
